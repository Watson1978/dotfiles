[alias]
	co = checkout
	br = branch
	st = status
	l = log --stat --pretty=format:'%Cblue%h %Cgreen%ar %Cred%an %Creset%s %Cred%d'
	short = log --pretty=format:\"%h - %an, %ar : %s\"
	tree = log --oneline --decorate --graph
	unstage = reset HEAD --
	last = log -1 HEAD
